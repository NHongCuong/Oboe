{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/__/auth/**",
        "function": "noop"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
